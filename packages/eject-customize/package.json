{
  "name": "@repo/eject-customize",
  "version": "0.1.0",
  "description": "Framework-agnostic ejection and customization system for Next.js + NestJS template",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./types": {
      "import": "./src/types/index.ts",
      "types": "./src/types/index.ts"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/tsconfig": "*",
    "@repo/vitest-config": "*",
    "@types/node": "catalog:build",
    "typescript": "catalog:build",
    "vitest": "catalog:testing"
  },
  "dependencies": {
    "zod": "catalog:utils"
  },
  "keywords": [
    "eject",
    "customize",
    "framework",
    "scaffolding",
    "turbo",
    "monorepo"
  ]
}
