{
  "name": "stats-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    " ": " ",
    "prettify": "bun x prettier . --write --no-error-on-unmatched-pattern",
    "dev": "bun --bun scripts/entrypoint.dev.ts",
    "prod": "bun --bun scripts/entrypoint.prod.ts",
    "dev:docker": "envcli cross-env NODE_OPTIONS='--inspect' next dev -H 0.0.0.0 -p $:{NEXT_PUBLIC_STATS_PORT} --turbopack",
    "build": "bun --bun scripts/build.ts",
    "build:no:check": "bun --bun generate && cross-env NO_CHECK=true next build",
    "generate": "bun --bun dr:build",
    "start": "envcli next start -p $:{NEXT_PUBLIC_STATS_PORT}",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf .next",
    "dr:build": "bun declarative-routing build",
    "dr:build:watch": "bun declarative-routing build --watch"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@hookform/resolvers": "catalog:utils",
    "@n0safe/envcli": "catalog:utils",
    "@repo-bin/declarative-routing": "*",
    "@repo/ui": "*",
    "clsx": "catalog:styling",
    "concurrently": "catalog:dev",
    "cross-env": "catalog:dev",
    "lucide-react": "catalog:ui",
    "next": "catalog:next",
    "nprogress": "^0.2.0",
    "query-string": "^9.3.1",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "react-hook-form": "catalog:react",
    "recharts": "^3.3.0",
    "sonner": "catalog:ui",
    "tw-animate-css": "^1.4.0",
    "yaml": "^2.8.1",
    "zod": "catalog:utils"
  },
  "devDependencies": {
    "@repo-configs/eslint": "*",
    "@repo-configs/prettier": "*",
    "@repo-configs/tailwind": "*",
    "@repo-configs/typescript": "*",
    "@repo-configs/vitest": "*",
    "@repo/types": "*",
    "@tailwindcss/postcss": "catalog:styling",
    "@testing-library/jest-dom": "catalog:testing",
    "@testing-library/react": "catalog:testing",
    "@testing-library/user-event": "catalog:testing",
    "@types/node": "catalog:build",
    "@types/nprogress": "^0.2.3",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@vitejs/plugin-react": "catalog:testing",
    "@vitest/coverage-v8": "catalog:testing",
    "@vitest/ui": "catalog:testing",
    "babel-plugin-react-compiler": "catalog:react-compiler",
    "dotenv": "catalog:dev",
    "dotenv-cli": "catalog:dev",
    "dotenv-expand": "catalog:dev",
    "eslint": "catalog:linting",
    "eslint-config-next": "catalog:next",
    "jsdom": "catalog:testing",
    "prettier": "catalog:linting",
    "prettier-plugin-tailwindcss": "catalog:linting",
    "rimraf": "catalog:build",
    "tailwind-merge": "catalog:styling",
    "tailwind-variants": "catalog:styling",
    "tailwindcss": "catalog:styling",
    "typescript": "catalog:build",
    "vitest": "catalog:testing"
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
